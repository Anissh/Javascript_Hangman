		document.getElementById("txt11").style.visibility="hidden";
		document.getElementById("txt12").style.visibility="hidden";  
	 } else if (theword.length == 9) {
		document.getElementById("txt10").style.visibility="hidden";
		document.getElementById("txt11").style.visibility="hidden";
		document.getElementById("txt12").style.visibility="hidden";   
	 } else if (theword.length == 10) {
		document.getElementById("txt11").style.visibility="hidden";
		document.getElementById("txt12").style.visibility="hidden";   
	 } else if (theword.length == 11) {
		 document.getElementById("txt12").style.visibility="hidden"; 
	 }

	if (numwrong == 9) {
	snd2.play()
	document.getElementById("imggallows").src="hang8.png"
	alert("You have lost the game!")
	alert(theword)
	rndloss++;
	restart();
	}
	if (numright == theword.length) {
	snd4.play()
	alert("You have won this round!")
	rndwin++;
	restart();
	}
	document.getElementById("rndwins").value=rndwin
	document.getElementById("rndlosses").value=rndloss
}











</script>
